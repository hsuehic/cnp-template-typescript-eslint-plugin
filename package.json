{
  "name": "eslint-plugin-xxx",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": ">=8.10.0"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "lint": "run-s lint:ts lint:eslint",
    "lint:ts": "tsc --noEmit",
    "lint:eslint": "eslint '**/*.{js,ts}'",
    "lint:prettier": "prettier '**/*.{js,ts,json,yaml,md}'",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "add-rule": "ts-node scripts/add-rule",
    "update": "ts-node scripts/update"
  },
  "peerDependencies": {
    "eslint": ">=8.45.0"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.2",
    "@typescript-eslint/experimental-utils": "^4.20.0"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.8",
    "@types/jest": "^26.0.22",
    "eslint": "^8.45.0",
    "eslint-config-cnp": "^0.0.3",
    "husky": "^8.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{json,yaml,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/???/eslint-plugin-xxxx.git"
  },
  "keywords": [
    "typescript",
    "template",
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/???/eslint-plugin-xxxx/issues"
  },
  "homepage": "https://github.com/???/eslint-plugin-xxxx#readme"
}
